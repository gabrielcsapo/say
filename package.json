  },
  "homepage": "https://github.com/gabrielcsapo/saywhat#readme",
  "devDependencies": {
    "ace-builds": "^1.2.6",
    "async": "^2.1.4",
    "browserify": "^14.1.0",
    "eslint": "^3.15.0",
    "gm": "^1.22.0",
    "grunt": "^1.0.1",
    "grunt-contrib-pug": "^1.0.0",
    "grunt-screenshot": "^0.6.0",
    "html2png": "^1.1.2",
    "jquery": "^3.1.1",
    "progress": "^1.1.8",
    "psychic-ui": "^0.4.0",
